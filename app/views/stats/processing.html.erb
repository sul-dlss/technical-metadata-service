<turbo-frame id="processing_stats">
  <table class="table table-striped">
  <tbody>
    <tr>
      <td>In last 10 minutes</td>
      <td><%= number_with_delimiter(DroFile.where(updated_at: (10.minutes.ago)..Time.zone.now).count) %></td>
    </tr>
    <tr>
      <td>In last hour</td>
      <td><%= number_with_delimiter(DroFile.where(updated_at: (1.hour.ago)..Time.zone.now).count) %></td>
    </tr>
    <tr>
      <td>In last day</td>
      <td><%= number_with_delimiter(DroFile.where(updated_at: (1.day.ago)..Time.zone.now).count) %></td>
    </tr>
    <tr>
      <td>In last week</td>
      <td><%= number_with_delimiter(DroFile.where(updated_at: (1.week.ago)..Time.zone.now).count) %></td>
    </tr>
  </tbody>
  </table>
</turbo-frame>
